<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.com.namo.apkstore.manager"
    android:versionCode="22"
    android:versionName="1.12" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/Transparent" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" android:screenOrientation="landscape">
        </activity>
        <activity android:name="ActivityLogin" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="HostSettingActivity"></activity>
        <activity android:name="ApkManagerActivity"></activity>
        <activity android:name="ActivityRadioSelect"></activity>
        <activity android:name="SocketUploadActivity"></activity>
        <activity android:name="ActivityCategorySelect"></activity>
        <activity android:name="ActivitySupplierSelect"></activity>
        <activity android:name="ActivityBrandSelect"></activity>
        <activity android:name="ActivityModelSelect"></activity>
        <activity android:name="ActivityCustomerApkManager"></activity>
        <activity android:name="ActivityCustomerSelect"></activity>
        <activity android:name="ActivityFileUpload"></activity>
    </application>



</manifest>